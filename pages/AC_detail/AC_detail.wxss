/* pages/AC_detail/AC_detail.wxss */
.line{
  width: 40%;
 height:15rpx;
  background:rgb(221, 221, 221);
  }

.uploadOut {
  background-color: #eeece9;
  position: relative;
}
.utilTop {
  background-color: #fff;
  width: 100%;
  height: 115rpx;
  line-height: 115rpx;
  border-top: 1px solid #eeece9;
  border-bottom: 1px solid #eeece9;
  padding-left: 405rpx;
  padding-right: 18rpx;
  position: fixed;
  top: 0;
  z-index: 2;
}
.van-button.van-button--info.van-button--small {
  border-radius: 9rpx;
  margin-left: 37rpx;
}
.articleTitle {
  margin-top: 115rpx;
}
.uploadRadio {
  background-color: #fff;
  border-bottom: 1px solid #eeece9;
  padding: 10rpx 10rpx 20rpx 20rpx;
}

.van-radio.van-radio--horizontal {
  margin-top: 20rpx;
}
.mineBody {
  background-color: #fff;
  height: calc(100vh + 5000px);
}
.btn{
  margin: 20rpx 50rpx;
  background-color: #D20A0A;
  color: white;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
}
.van-popup--bottom.van-popup--safe {
  padding: 10px 10px 6px;
}
.van-icon.van-icon-cross {
  top: -18rpx;
  left: 21rpx;
} 

.Building {
  background-color: rgb(255, 255, 255);
  margin-left: 30rpx;
  font-size: 28rpx;
  height: 90rpx;
}

.Building input {
  margin: auto 0rpx;
}

.room {
font-size: 28rpx;
color: rgb(3, 3, 3);
}
.placeholder{
  font-size: 27rpx;
  color: black;
}


@import "../components/wxParse/wxParse.wxss"